<html>
  <head>
    <link rel="stylesheet" href="playliststyle.css"/>
    <link
      id="favicon"
      rel="icon"
      href="https://i.ibb.co/WW98p14/Project-Drawing-16310933759254199759.png"
      type="image/x-icon"
    />
  </head>
  <body>
    <h1>
      Download Youtube Playlists
      <h3>
        with <a href="/">tattered-lead-archaeopteryx.glitch.me</a>
      </h3>
    </h1>
    <input id="playlistURLContainer"/>
    <div id="playlistContainer">
      <%= JSON.stringify(contents.playlistTitle)%>
      <!--<img id="initialLoading" src="https://wpamelia.com/wp-content/uploads/2018/11/loading-hourglass.gif"/>-->
      <% contents.contents.forEach((playlistContent) => { %>
        <div class="playlistContentContainer">
          <img src=<% playlistContent[0].playlistVideoRenderer.thumbnail.thumbnails[0].url %> class="playlistContentThumbnail"/>
        </div>
      <% }); %>
    </div>
    <!--<script src="playlistscript.js"></script>-->
  </body>
</html>